{
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store, max-age=0"
        },
        {
          "key": "Connection",
          "value": "keep-alive"
        }
      ]
    }
  ],
  "functions": {
    "src/app/api/**/*": {
      "memory": 1024,
      "maxDuration": 30
    }
  }
}